        </div>
      </section>
    </div>
  </header>


  <!-- Sign Up Section -->
  <section class="page-section portfolio" id="today-segment">
    <div class="container">

      <!-- Sign Up Section Heading -->
      <h2 class="page-section-heading text-center text-uppercase text-secondary mb-0">Sign In to your Admin Account</h2>

      <!-- Icon Divider -->
      <div class="divider-custom">
        <div class="divider-custom-line"></div>
        <div class="divider-custom-icon">
          <i class="fas fa-star"></i>
        </div>
        <div class="divider-custom-line"></div>
      </div>

      <br>
      <p class="lead"><a href="signup">Havenâ€™t got an account yet? <u>Sign up here.</u></a></p>
      <form action="/login" method="POST" id="register">
        <div class="form-group">
          <label class="lead" for="emailAddress">Email Address</label>
          <input type="email" class="form-control" id="emailAddress" name="username" aria-describedby="emailHelp" placeholder="Enter email" required>
        </div>
        <div class="form-group">
          <label class="lead" for="password">Password</label>
          <input type="password" class="form-control" id="password" name="password" placeholder="Password" required>
          <small id="passwordInvalid" class="form-text text-muted"><a href="#club-id-section"></a></small>
          <p class="lead"><a href="forgot-password"><u>Forgotten Password?</u></a></p>
        </div>
        <p class="lead"><%-invalidPassword%></p>
        <button type="submit" class="btn btn-lg btn-primary">Log In</button>
      </form>
    </div>
  </section>

